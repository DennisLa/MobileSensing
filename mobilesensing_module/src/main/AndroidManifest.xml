<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="de.dennis.mobilesensing_module">

    <!-- Required for Intel Sensing -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.READ_CALL_LOG" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.GET_TASKS" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <!-- Required for Intel Architecture -->
    <uses-permission android:name="com.google.android.gms.permission.ACTIVITY_RECOGNITION" />
    <uses-permission android:name="intel.aware.permission.ACCESS_AWARESERVICE" />

    <application>
        android:name=".mobilesensing.Module"
        android:allowBackup="false"
        android:label="@string/app_name"
        android:supportsRtl="true">
        <activity android:name=".mobilesensing.PermissionActivity"></activity>

        <service android:name="com.intel.context.core.LocalService" />
        <receiver android:name="de.dennis.mobilesensing_module.mobilesensing.Sensors.OtherSensors.RunningApplicationService.RunningApplicationListener" />
        <receiver android:name="de.dennis.mobilesensing_module.mobilesensing.Upload.DailyUpload.UploadListener" />
        <receiver android:name="de.dennis.mobilesensing_module.mobilesensing.Sensors.OtherSensors.ScreenOnService.ScreenOnListener"/>
        <receiver android:name="de.dennis.mobilesensing_module.mobilesensing.Sensors.GoogleActivity.GActivityListener"/>
        <receiver android:name="de.dennis.mobilesensing_module.mobilesensing.Sensors.GoogleActivity.GActivityTransitionListener"/>

        <service
            android:name="de.ms.ptenabler.locationtools.ClusterService"
            android:enabled="true" />

        <service
            android:name="de.dennis.mobilesensing_module.mobilesensing.Sensors.OtherSensors.ClusterService.ClusterJobService"
            android:enabled="true"
            android:permission="android.permission.BIND_JOB_SERVICE"/>

    </application>


</manifest>